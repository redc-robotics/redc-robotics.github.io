<div class="wrapper form">
  <h1 class="form_header">Log In</h1>

  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email, class: "form_title" %>
    <%= f.text_field :email %>

    <%= f.label :password, class: "form_title" %>
    <%= f.password_field :password %>

    <!--
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me, class: "form_title checkbox_form" %>
  -->

    <%= f.submit "Sign in" %>
  <% end %>
</div>
